<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="action_product_line" model="ir.actions.act_window">
        <field name="name">Product</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">product.product</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>

    <menuitem id="menu_products"
              name="Product"
              parent="menu_purchase_orf_root"
              action="action_product_line"/>


    <record id="action_vendor_line" model="ir.actions.act_window">
        <field name="name">Vendor</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>

    <menuitem id="menu_vendor_line"
              name="Vendor"
              parent="menu_purchase_orf_root"
              action="action_vendor_line"
              sequence="30"/>

</odoo>